{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "phoenix-app": {
      "root": "projects/phoenix-app",
      "sourceRoot": "projects/phoenix-app/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/phoenix-app",
            "index": "projects/phoenix-app/src/index.html",
            "main": "projects/phoenix-app/src/main.ts",
            "polyfills": "projects/phoenix-app/src/polyfills.ts",
            "tsConfig": "projects/phoenix-app/tsconfig.app.json",
            "assets": [
              "projects/phoenix-app/src/favicon.ico",
              "projects/phoenix-app/src/assets",
              "projects/phoenix-app/src/api-docs"
            ],
            "styles": [
              "projects/phoenix-app/src/styles.scss"
            ],
            "allowedCommonJsDependencies": [
              "css-element-queries",
              "@tweenjs/tween.js"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/phoenix-app/src/environments/environment.ts",
                  "with": "projects/phoenix-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "phoenix-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "phoenix-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "phoenix-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/phoenix-app/src/test.ts",
            "polyfills": "projects/phoenix-app/src/polyfills.ts",
            "tsConfig": "projects/phoenix-app/tsconfig.spec.json",
            "karmaConfig": "projects/phoenix-app/karma.conf.js",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "projects/phoenix-app/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/phoenix-app/src/favicon.ico",
              "projects/phoenix-app/src/assets",
              "projects/phoenix-app/src/api-docs"
            ],
            "codeCoverageExclude": [
              "projects/phoenix-app/src/app/services/extras/**"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/phoenix-app/src/tsconfig.app.json",
              "projects/phoenix-app/src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "phoenix-app-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "phoenix-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "phoenix-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "phoenix-ui": {
      "projectType": "library",
      "root": "projects/phoenix-ui",
      "sourceRoot": "projects/phoenix-ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/phoenix-ui/tsconfig.lib.json",
            "project": "projects/phoenix-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/phoenix-ui/tsconfig.lib.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/phoenix-ui/src/test.ts",
            "tsConfig": "projects/phoenix-ui/tsconfig.spec.json",
            "karmaConfig": "projects/phoenix-ui/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/phoenix-ui/tsconfig.lib.json",
              "projects/phoenix-ui/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "phoenix-app"
}
